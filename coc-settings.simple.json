{
    "coc.preferences.extensionUpdateCheck": "daily",
    "coc.preferences.currentFunctionSymbolAutoUpdate": true,
    "coc.preferences.formatOnType": true,
    "coc.preferences.formatOnSave": true,
    "snippet.highlight": true,
    "suggest.labelMaxLength": 60,
    "suggest.noselect": true,
    "suggest.enablePreselect": false,
    "suggest.triggerAfterInsertEnter": true,
    "suggest.autoTrigger": "always",
    "suggest.timeout": 5000,
    "suggest.snippetIndicator": "",
    "suggest.triggerCompletionWait": 50,
    "suggest.completionItemKindLabels": {
        "class": "\ueb5b",
        "color": "\ueb5c",
        "constant": "\ueb5d",
        "default": "\uf29c",
        "enum": "\uea95",
        "enumMember": "\ueb5e",
        "event": "\uea86",
        "field": "\ueb5f",
        "file": "\uea7b",
        "folder": "\uf115",
        "function": "\u0192\udb80\ude95",
        "interface": "\uf417",
        "keyword": "\ueb62",
        "method": "\uea8c",
        "module": "\uea8b",
        "operator": "\ueb64",
        "property": "\ue624",
        "reference": "\ueb36",
        "snippet": "\ue60b",
        "struct": "\uea91",
        "text": "\ue612",
        "typeParameter": "\ueb66",
        "unit": "\uf475",
        "value": "\uf4f7",
        "variable": "\uea88"
    },
    "diagnostic.errorSign": "✘",
    "diagnostic.warningSign": "⚠",
    "diagnostic.infoSign": "⋆",
    "diagnostic.hintSign": "⋅",
    "diagnostic.refreshOnInsertMode": false,
    "diagnostic.checkCurrentLine": true,
    "diagnostic.virtualText": true,
    "codeLens.enable": true,
    "list.normalMappings": {
        "<C-n>": "normal:j",
        "<C-p>": "normal:k",
        "t": "action:tabe",
        "v": "action:vsplit",
        "s": "action:split",
        "d": "expr:GetDeleteAction"
    },
    "list.insertMappings": {
        "<CR>": "do:defaultaction",
        "<C-r>": "do:refresh",
        "<C-g>": "do:switch",
        "<C-n>": "normal:j",
        "<C-p>": "normal:k",
        "<C-t>": "action:tabe",
        "<C-v>": "action:vsplit",
        "<C-s>": "action:split",
        "<C-w>": "command:wincmd k"
    },
    "list.previewHighlightGroup": "Statement",
    "git.addedSign.hlGroup": "GitGutterAdd",
    "git.changedSign.hlGroup": "GitGutterChange",
    "git.removedSign.hlGroup": "GitGutterDelete",
    "git.topRemovedSign.hlGroup": "GitGutterDelete",
    "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
    "git.addGBlameToVirtualText": false,
    "git.addGBlameToBufferVar": true,
    "emmet.includeLanguages": {
        "vue-html": "html"
    },
    "tsserver.enable": true,
    "tsserver.log": "normal",
    "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
    "eslint.debug": true,
    "eslint.enable": true,
    "eslint.packageManager": "yarn",
    "eslint.format.enable": true,
    "eslint.autoFixOnSave": true,
    "eslint.probe": [
        "vue",
        "html",
        "json",
        "jsonc",
        "javascript",
        "typescript"
    ],
    "eslint.validate": [
        "vue",
        "json",
        "html",
        "javascript",
        "typescript"
    ],
    "eslint.options": {},
    "html.filetypes": [
        "html",
        "xhtml"
    ],
    "html.format.wrapLineLength": 100,
    "html.trace.server": "verbose",
    "html.format.indentInnerHtml": false,
    "html.format.extraLiners": "head, body, /html",
    "html.format.unformatted": "no-tag",
    "html.format.enable": true,
    "json.format.enable": true,
    "yaml.format.enable": true,
    "yaml.format.proseWrap": "always",
    "yaml.format.printWidth": 120,
    "pyright.organizeimports.provider": "isort",
    "pyright.inlayHints.functionReturnTypes": true,
    "python.sortImports.path": "isort",
    "pyright.inlayHints.variableTypes": false,
    "pyright.inlayHints.parameterTypes": false,
    "python.sortImports.args": [
        "--length-sort",
        "--dont-order-by-type",
        "--force-single-line-imports"
    ],
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "--line-length=100"
    ],
    "python.formatting.autopep8Args": [
        "--max-line-length=100"
    ],
    "python.linting.enabled": true,
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Args": [
        "--max-complexity=9",
        "--max-doc-length=100",
        "--max-line-length=100",
        "--ignore=E203,W503",
        "--exclude=.git,__pycache__,old,build,dist"
    ],
    "python.linting.pylintEnabled": true,
    "python.linting.pytypeEnabled": false,
    "python.linting.pycodestyleEnabled": false,
    "python.linting.pylintArgs": [
        "--disabled=W0120,useless-else-on-loop"
    ],
    "python.pythonPath": "python",
    "python.analysis.typeshedPaths": [
        "~/.pyright/typeshed/"
    ],
    "snippets.ultisnips.directories": [
        "$HOME/.cache/nvimfiles/plugins/vim-snippets/UltiSnips/",
        "$HOME/.config/vimrc/snippets/UltiSnips/"
    ],
    "snippets.massCode.enable": false,
    "yank.enableCompletion": true,
    "yank.list.maxsize": 999,
    "yank.limit": 9,
    "yank.priority": 10,
    "languageserver": {}
}
